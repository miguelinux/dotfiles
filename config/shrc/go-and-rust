GOPATH=${HOME}/go
PATH=${PATH}:${GOPATH}/bin:${HOME}/.cargo/bin

if [ -f "$HOME/.cargo/env" ]
then
    source "$HOME/.cargo/env"
fi

export GOPATH PATH
